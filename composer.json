{
    "name": "quinninteractive/silverstripe-webp-substitution",
    "description": "Automatically convert images to WebP format",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "image",
        "images",
        "WebP"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.4",
        "nette/finder": "^2.5",
        "rosell-dk/webp-convert": "^2.6"
    },
    "conflict": {
        "silverstripe/s3": "*"
    },
    "autoload": {
        "psr-4": {
            "QuinnInteractive\\WebPSub\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
